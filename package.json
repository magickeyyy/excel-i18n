{
    "name": "excel-i18n",
    "version": "1.0.0",
    "description": "从按照预期格式的EXCEL文件中某一页提取多语言文本，生成json、ts、js文件",
    "main": "src/index.ts",
    "repository": "https://gitee.com/magickeyyy/excel2json.git",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "test": "node test",
        "prettier": "prettier --write --ignore-unknown",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "keywords": [
        "excel",
        "i18n",
        "xlsx",
        "csv",
        "sheetjs"
    ],
    "author": "magickeyyy",
    "license": "ISC",
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "colors": "^1.4.0",
        "xlsx": "^0.17.4"
    },
    "devDependencies": {
        "@rollup/plugin-inject": "^4.0.3",
        "@rollup/plugin-typescript": "^8.3.0",
        "@tsconfig/node12": "^1.0.9",
        "@types/node": "^16.11.10",
        "chai": "^4.3.4",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "mocha": "^9.1.3",
        "nodemon": "^2.0.15",
        "prettier": "2.5.0",
        "rollup": "^2.60.2",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript2": "^0.31.1",
        "ts-lib": "0.0.5",
        "ts-node": "10.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.2"
    }
}
